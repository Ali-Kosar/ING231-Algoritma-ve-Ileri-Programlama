#alıştırmalar 1, soru 9

a=range(1,1000)
for i in a:
    i=str(i)
    if int(i)<10:
        if int(i[0])<9:
            print(i,end=" ")
    elif 9<int(i)<100:
        if int(i[0])+int(i[1])<9:
            print(i,end=" ")
    else:
        if int(i[0])+int(i[1])+int(i[2])<9:
            print(i,end=" ")
